@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .

<http://opendata.inrae.fr/project/8wDYkRBgfO4riwsCEi9u0E> a owl:Ontology .
skos:example a owl:AnnotationProperty .
<http://opendata.inrae.fr/BioinformaticWorkflow>
  a owl:Class ;
  rdfs:comment "L'ensemble des outils bioinformatique, leur enchainement et leurs paramètres, utilisé pour traiter les données de séquencage ADN brutes"@fr, "The set of bioinformatics tools, their sequence and parameters, used to process the raw DNA sequencing data."@en ;
  rdfs:label "BioinformaticWorkflow"@en .

<http://opendata.inrae.fr/ExtractionAndSequencingProcedures>
  a owl:Class ;
  rdfs:comment "A procedure used either for the nucleic acid extraction, the PCR amplification, the library preparation, or the sequecing."@en ;
  rdfs:label "ExtractionAndSequencingProcedure"@en .

<http://opendata.inrae.fr/MonitoredParametersValues>
  a owl:Class ;
  rdfs:comment "a"^^xsd:string ;
  rdfs:label "MonitoredParametersValues" .

<http://opendata.inrae.fr/PCRconditions>
  a owl:Class ;
  rdfs:comment "a"^^xsd:string ;
  rdfs:label "PCRconditions"@en .

<http://opendata.inrae.fr/Coordinator>
  a owl:Class ;
  owl:equivalentClass [
    a owl:Restriction ;
    owl:onProperty <http://opendata.inrae.fr/hasCoordinator> ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger
  ] ;
  rdfs:label "Coordinator" .

<http://opendata.inrae.fr/ProjectStatus>
  a owl:Class ;
  rdfs:label "ProjectStatus" .

<http://opendata.inrae.fr/R0vQz5v5oAWsQqDrRJiEmk>
  a owl:Class ;
  rdfs:comment "a"^^xsd:string ;
  rdfs:label "SequencingSample"@en .

<http://opendata.inrae.fr/R31IIiJsQgdXVd9R1V3JzK>
  a owl:Class ;
  rdfs:comment "Process with a full-scale or located on the field, as opposed to pilot reactors and laboratory reactors."@en ;
  rdfs:label "FieldOrFullScaleProcess"@en .

<http://opendata.inrae.fr/R7i1g5l24IgUPk47WkCbXl6>
  a owl:Class ;
  rdfs:comment "Reactor of small scale or pilot scale, as opposed to full-scale and field processes. The laboratory reactors are intended to be very precisely described, in terms of design and operation (volume, temperature, inputs, etc)."@en ;
  rdfs:label "LaboratoryReactor"@en .

<http://opendata.inrae.fr/R8L7Tt57H5wARQtTc2lB5j2>
  a owl:Class ;
  rdfs:comment "A quantitative parameter monitored either in a compartment of a laboratory reactor, or in an industrial process (e.g. pH, cumulated biogas production, qPCR analysis of total bacteria, ...)."@en ;
  rdfs:label "MonitoredParameter"@en .

<http://opendata.inrae.fr/R8ZfXvfOenQJl0Iy5anZJo>
  a owl:Class ;
  rdfs:comment "Persons who have access to a project and all its data and metadata, with read access only."@en ;
  rdfs:label "Observers"@en .

<http://opendata.inrae.fr/R9Bpeod8RWi83G6WMQdgd4>
  a owl:Class ;
  rdfs:comment "It represents a sequencing run, in this precise case for amplicon sequencing. It is thus described with its main characteristics (such as the sequencing apparatus) and it gathers all the biosamples from the considered project sequenced during the same run."@en ;
  rdfs:label "MetabarcodingRun"@en .

<http://opendata.inrae.fr/R9ECpaLRdJqqYcsc1SNHYI0>
  a owl:Class ;
  rdfs:comment "a"^^xsd:string ;
  rdfs:label "ProcessCategory"@en .

<http://opendata.inrae.fr/R9lqY9PvvxLPOdsfkH3IpmT>
  a owl:Class ;
  rdfs:comment "It represents a physical sample for which you plan to perform a specific meta-omics analysis."@en ;
  rdfs:label "Biosample"@en ;
  skos:example "It is advised to create them in DeepOmics before the acquisition of the corresponding meta-omics data. If two different meta-omics analyses will be performed on a same physical samples, two distinct biosamples need to be created."@en .

<http://opendata.inrae.fr/R9zv4zizmMYQ4Ri25GOdBbZ>
  a owl:Class ;
  rdfs:comment "Reactors which were subjected to the exact same treatment; they are grouped by operating conditions."@en ;
  rdfs:label "Replicate"@en .

<http://opendata.inrae.fr/RBFDF1ED2ddpRUQeraazOTB>
  a owl:Class ;
  rdfs:comment "a"^^xsd:string ;
  rdfs:label "Participants"@en .

<http://opendata.inrae.fr/RCJHjWRnseBqph8bD9zewGF>
  a owl:Class ;
  rdfs:comment "They represent a consistent batch of experiments led into reactors or pilots, within a project. They are structured into operating conditions and replicates."@en ;
  rdfs:label "ExperimentalSeries"@en .

<http://opendata.inrae.fr/RCVoiubGarAjlzSe2KaZQ85>
  a owl:Class ;
  rdfs:comment "a"^^xsd:string ;
  rdfs:label "Operators"@en .

<http://opendata.inrae.fr/RCdwDIy9ZQUXq2nO2AU8Nbj>
  a owl:Class ;
  rdfs:comment "a"^^xsd:string ;
  rdfs:label "BiosampleSequencingRawData"@en .

<http://opendata.inrae.fr/RD2zZxblOdtm9rug8MSF5SW>
  a owl:Class ;
  rdfs:comment "Voici la description"^^xsd:string ;
  rdfs:label "SequencingRun"@en .

<http://opendata.inrae.fr/RDiOpHJWqpm6LhZzk8jZiJu>
  a owl:Class ;
  rdfs:comment "a"^^xsd:string ;
  rdfs:label "BiosampleSequencingProcessedData"@en .

<http://opendata.inrae.fr/RDye55BBTTvSE3XSpskCyyO>
  a owl:Class ;
  rdfs:comment "It corresponds to the industrial site from which the samples originate (e.g: a wastewater treatment plant)."@en ;
  rdfs:label "SamplingSite"@en .

<http://opendata.inrae.fr/RGqfVMSr7brs5umWLFqja9>
  a owl:Class ;
  rdfs:comment "It corresponds to the process and reactor from which the samples from the field originate. Indeed, a single industrial site can gather several processes (e.g. activated sludge, anaerobic digester) and the reactor from which the sample originates needs to be specified."@en ;
  rdfs:label "BiotechnologicalProcess"@en .

<http://opendata.inrae.fr/RHbmcqQacw0ZATMxnsbYBd>
  a owl:Class ;
  rdfs:comment "The inputs are the compounds and elements which will be used in the reactors (feeding, gas, inocula, buffers, matrix, pure microbial strain, …). They are described according to a controlled vocabulary and they are defined at the scale of the project."@en ;
  rdfs:label "Input"@en .

<http://opendata.inrae.fr/RJVWzyLN46xRFZUDi6vZPw>
  a owl:Class ;
  rdfs:comment "a"^^xsd:string ;
  rdfs:label "SamplingCampaign"@en .

<http://opendata.inrae.fr/RY0uXvXQWLncugFW7GQRq8>
  a owl:Class ;
  rdfs:comment "A physical compartment of a laboratory reactor. Typically, bioelectrochemical system reactors can have several compartments, such as the anodic compartment and the cathodic compartment."@en ;
  rdfs:label "Compartment"@en .

<http://opendata.inrae.fr/Project>
  a owl:Class ;
  rdfs:comment "a"^^xsd:string ;
  skos:closeMatch "http://www.opensilex.org/vocabulary/oeso#Project" ;
  rdfs:label "Project"@en .

<http://opendata.inrae.fr/Rfb9ON3BGs2PyxVSO1xyQa>
  a owl:Class ;
  rdfs:comment "It describes the conditions and treatment applied to a given number of replicated reactors. Each reactor can be composed of 1 to 3 distinct compartments. A given operating condition can be defined at the level of the reactor (if it is identical for all compartments) or at the level of each compartment. In each operating condition, you will be able to define replicates."@en ;
  rdfs:label "OperatingCondition"@en .

<http://opendata.inrae.fr/RqbmhyT5wQ7YNnTWhLhKd3>
  a owl:Class ;
  rdfs:comment "a"^^xsd:string ;
  rdfs:label "Reactor"@en .

<http://opendata.inrae.fr/TaxonAnnotation>
  a owl:Class ;
  rdfs:comment "a"^^xsd:string ;
  rdfs:label "TaxonAnnotation" .

<http://opendata.inrae.fr/TaxonCount>
  a owl:Class ;
  rdfs:comment "a"^^xsd:string ;
  rdfs:label "TaxonCount" .

<http://opendata.inrae.fr/hasCoordinator>
  a owl:ObjectProperty ;
  owl:inverseOf <http://opendata.inra.fr/PO2/isCoordinatedBy> ;
  rdfs:domain <http://opendata.inrae.fr/Project> ;
  rdfs:range foaf:Agent ;
  rdfs:label "Has Coordinator"@en ;
  rdfs:comment "Link to express that a project is coordinated by an agent"@en .